FoamFile
    {
        version     2.0;
        format      ascii;
        class       volScalarField;
        object      Phi;
    }

    dimensions      [0 2 -1 0 0 0 0];

    internalField   uniform 0;

    boundaryField
    {
        velocity_inlet
        {
            type            fixedValue;
            value           uniform 1.0;
        }
        
        pressure_outlet
        {
            type            fixedValue;
            value           uniform 0.0;
        }
        
        walls
        {
            type            slip;
        }
    }
    
